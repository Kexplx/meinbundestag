{
  "expo": {
    "androidStatusBar": {
      "backgroundColor": "#D08770"
    },
    "name": "meinbundestag-mobile",
    "slug": "meinbundestag-mobile",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "sourceExts":  [ "js", "json", "ts", "tsx", "jsx", "vue"]
    },
    "entryPoint": "./src/App.js"
  }
}
